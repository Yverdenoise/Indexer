package cimav.sgc.search;

import java.util.ArrayList;
import java.util.List;

/**
 *
 * @author calderon
 */
public class Documento {
        public String depto;
        public String tipo;
        public String codigo;
        public String nombre;
        public String contenido;
        public String codigosReferenciados;
        public List<String> fragmentos;

        public Documento(String depto, String tipo, String codigo, String nombre, String contenido, String codigosReferenciados) {
            this.depto = depto;
            this.tipo = tipo;
            this.codigo = codigo;
            this.nombre = nombre;
            this.contenido = contenido;
            this.codigosReferenciados = codigosReferenciados;
            this.fragmentos = new ArrayList<>();
        }        
    
}
